{
  "manifest_version": 2,

  "name": "Altran SP",
  "description": "Check for updates on the intranet.",
  "version": "0.0.3",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
    
  "background": {
    "scripts": ["jquery.min.js","background.js"]
  },
  "permissions": [
    "https://altranintranet.sharepoint.com/",
    "storage"
  ]
}
